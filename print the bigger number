#include <stdio.h>
int main() {
	int a, b, c;
	printf ("eisigage ton proto arithmo ");
	scanf ("%d", &a);
	printf ("eisigage ton deutero arithmo ");
	scanf ("%d", &b);
	printf ("eisigage ton trito arithmo ");
	scanf ("%d", &c);
	if (a>=b && a>=c)
	printf ("%d megaliteros arithmos ",a );
	if (b>=a && b>=c)
	printf ("%d megaliteros arithmos ",b );
	if (c>=a && c>=b)
	printf ("%d megaliteros arithmos ",c );
	
	return 0;
}
